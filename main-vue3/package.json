{
  "name": "dnhyxc",
  "private": true,
  "version": "0.0.1",
  "author": "dnhyxc",
  "description": "a web app",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "rimraf dist release && vue-tsc --noEmit && cross-env NODE_ENV=production && vite build",
    "build:vue": "rimraf dist release && vue-tsc --noEmit && cross-env NODE_ENV=production && vite build",
    "preview": "vite preview",
    "eslint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "prettier": "prettier --write",
    "commit": "git-cz",
    "prepare": "husky install",
    "test": "npx eslint ./src  --ext ts,vue,js --fix"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,vue}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@kangc/v-md-editor": "^2.3.15",
    "crypto-js": "^4.1.1",
    "echarts": "^5.3.3",
    "echarts-wordcloud": "^2.1.0-beta",
    "element-plus": "^2.2.28",
    "isomorphic-fetch": "^3.0.0",
    "js-md5": "^0.7.3",
    "moment": "^2.29.4",
    "pinia": "^2.0.29",
    "prismjs": "^1.29.0",
    "qrcode": "^1.5.1",
    "query-string": "^7.1.1",
    "vue": "^3.2.45",
    "vue-cropper": "^1.0.6",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/crypto-js": "^4.1.1",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/js-md5": "^0.7.0",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "commitizen": "^4.2.6",
    "commitlint-config-cz": "^0.13.3",
    "consola": "^2.15.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-define-config": "^1.12.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.12.2",
    "unplugin-element-plus": "^0.4.1",
    "unplugin-vue-components": "^0.23.0",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.11"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
